<h1 class="text-center mb-4">Featured Games</h1>
<div class="row row-cols-1 row-cols-md-3 g-4">
  <% data.forEach(element => { %>
      <div class="col">
        <div class="card h-100">
          <div class="card-body">
            <h5 class="card-title"><%= element.name %></h5>
            <p class="card-text">$<%= element.price %></p>
          </div>
        </div>
      </div>
  <% }) %>
</div>